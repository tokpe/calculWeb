<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	
	<servlet>
		<servlet-name>CalculServlet</servlet-name>
		<display-name>CalculServlet</display-name>
		<description></description>
		<servlet-class>sn.objis.calculweb.presentation.CalculServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ChargementStyle</servlet-name>
		<display-name>ChargementStyle</display-name>
		<description></description>
		<servlet-class>sn.objis.calculweb.presentation.ChargementStyle</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CalculServlet</servlet-name>
		<url-pattern>/calcul</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ChargementStyle</servlet-name>
		<url-pattern>/chargementStyle</url-pattern>
	</servlet-mapping>
</web-app>
